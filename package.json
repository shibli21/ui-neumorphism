{
  "name": "ui-neumorphism",
  "version": "1.1.3",
  "description": " React components library for neumorphism UI/UX trend.",
  "author": "AKAspanion",
  "license": "MIT",
  "repository": "AKAspanion/ui-neumorphism",
  "keywords": [
    "ui",
    "react",
    "reactjs",
    "react-js",
    "neumorphic",
    "neumorphism",
    "skeuomorphism",
    "ui-neumorphism",
    "neumorphism-ui"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs && npm run build:types",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "build:types": "copyfiles -f ./src/index.d.ts dist",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "@types/react": "^18.0.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^3.4.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/node": "^18.15.11",
    "@types/react-dom": "^18.0.11",
    "react-transition-group": "^4.4.5",
    "typescript": "^5.0.3"
  }
}